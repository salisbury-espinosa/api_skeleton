{
  "name": "btcapi",
  "version": "1.0.0",
  "description": "Btc api",
  "author": "Salisbury Espinosa",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "cd build && node main",
    "compile": "node build.js && backpack build",
    "dev": "node build.js && backpack dev | pino -o 'short' && npm start",
    "debug": "node build.js && backpack dev --inspect-brk | pino -o 'short'",
    "test": "node build.js && _mocha --compilers js:babel-register --exit",
    "lint": "eslint './server' './test'",
    "lintfix": "eslint --fix './server' './test'",
    "flow": "flow",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again"
  },
  "moduleRoots": [
    "server"
  ],
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "helmet": "^3.12.0",
    "pino": "^4.15.3",
    "response-time": "^2.3.2",
    "source-map-support": "^0.5.4",
    "swagger": "^0.7.5",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "swagger-jsdoc": "^1.9.7"
  },
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "backpack-core": "^0.7.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.68.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.2",
    "mocha": "^5.0.5",
    "prettier": "1.11.1",
    "shelljs": "^0.8.1",
    "sinon": "^4.5.0",
    "supertest": "^3.0.0"
  }
}
